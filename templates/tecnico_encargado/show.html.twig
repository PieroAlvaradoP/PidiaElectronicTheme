{% set _title = 'Configuraci√≥n' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'tecnico_encargado_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
